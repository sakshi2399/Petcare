<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hello">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/picture"
        android:label="Petcare"
        android:roundIcon="@mipmap/picture"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".guest_my_pets"></activity>
        <activity android:name=".pet" />
        <activity
            android:name=".fragment_vets"
            android:label="@string/app_name" />
        <activity android:name=".fragment_shops" />
        <activity
            android:name=".fragment_pets"
            android:label="@string/app_name" />
        <activity android:name=".tab_bg" />
        <activity android:name=".HomePage" />
        <activity android:name=".Registeration" />
        <activity android:name=".General" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPetFormActivity" />
        <activity android:name=".NgoMyPetsActivity" />
    </application>

</manifest>